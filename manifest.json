{
  "manifest_version": 3,
  "name": "Oxford Dictionary Extension",
  "version": "1.0",
  "description": "Search quickly vocabulary on Oxford Dictionary main page",
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "16": "images/logo.png",
    "32": "images/logo.png",
    "64": "images/logo.png",
    "128": "images/logo.png"
  },
  "background": {
    "service_worker": "scripts/content.js"
  }
}